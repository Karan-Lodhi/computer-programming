# Write a Python Program to Count and Filter Odd and Even Numbers of Given List Using Loops.

numbers = list(map(int, input().split()))

odd_count = 0
even_count = 0
odd_numbers = []
even_numbers = []

for num in numbers:
    if num % 2 == 0:
        even_count += 1
        even_numbers.append(num)
    else:
        odd_count += 1
        odd_numbers.append(num)

print(f"Odd numbers: {odd_numbers} (Count: {odd_count})")
print(f"Even numbers: {even_numbers} (Count: {even_count})")
